gameloaded = false

while gameloaded == false do
  wait()
  pcall(function()
    
    if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart ~= nil then
      gameloaded = true
    end

  end)
end

wait(2)

local decalsyeeted = false -- Leaving this on makes games look shitty but the fps goes up by at least 20.
    local g = game
    local w = g:GetService("Workspace")
    local l = g:GetService("Lighting")
    local t = w.Terrain
    t.WaterWaveSize = 0
    t.WaterWaveSpeed = 0
    t.WaterReflectance = 0
    t.WaterTransparency = 0
    l.GlobalShadows = false
    l.FogEnd = 9e9
    l.Brightness = 0
    pcall(function() settings().Rendering.QualityLevel = "Level01" end)
    for i,v in pairs(g:GetDescendants()) do
       if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
           v.Material = "Plastic"
    v.Reflectance = 0
    elseif v:IsA("Decal") and decalsyeeted then
    v.Transparency = 1
    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
    v.Lifetime = NumberRange.new(0)
    elseif v:IsA("Explosion") then
    v.BlastPressure = 1
    v.BlastRadius = 1
       end
    end
    for i,e in pairs(l:GetChildren()) do
    if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
    e.Enabled = false
    end
    end
